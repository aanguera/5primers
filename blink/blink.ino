//******  Includes  ******************************************************************



//******  Variables  *****************************************************************
// Ubicacio del led a la sortida 2
int led_placa = 13;


//******  Includes  ******************************************************************
void setup() {                
  pinMode(led_placa, OUTPUT);     
}



//******  Loop  **********************************************************************
void loop() {
  digitalWrite(led_placa, HIGH);
  delay(500);
  digitalWrite(led_placa, LOW);
  delay(500);
}
