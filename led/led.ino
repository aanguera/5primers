//******  Includes  ******************************************************************



//******  Variables  *****************************************************************
// Ubicacio del led a la sortida 2
int led_verd = 2;


//******  Includes  ******************************************************************
void setup() {                
  pinMode(led_verd, OUTPUT);     
}



//******  Loop  **********************************************************************
void loop() {
  digitalWrite(led_verd, HIGH);
  delay(500);
  digitalWrite(led_verd, LOW);
  delay(500);
}
